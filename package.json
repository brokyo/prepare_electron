{
  "name": "vibrations",
  "version": "0.7.1",
  "description": "the thought goes nowhere",
  "private": true,
  "scripts": {
    "build:web": "rm -r build/* && webpack",
    "electron": "electron .",
    "web:prod": "node app/server/server.js",
    "web:dev": "nodemon app/server/server.js",
    "build": "electron-builder build --mac --dir --publish never",
    "deploy": "electron-builder build --mac --publish always"
  },
  "build": {
    "appId": "vibrations",
    "mac": {
      "category": "awake.awake.awake.awake"
    }
  },
  "postinstall": "electron-builder install-app-deps",
  "repository": {
    "type": "git",
    "url": "https://github.com/brokyo/vibrations"
  },
  "author": "apc",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "electron": "^8.0.1",
    "electron-builder": "^22.3.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "electron-updater": "^4.2.2",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "node-hue-api": "^4.0.5"
  }
}
